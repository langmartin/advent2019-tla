(* --algorithm manhattan
variable Points1 = {}, Points2 = {}, i = 0, x = 0, y = 0, pts = {}, wire
begin

ONE:
	       wire := wire1;
	       goto LP;
	       \* Points1 := pts;

	       \* wire := wire2;
	       \* goto LP;
	       \* Points2 := pts;
	       \* skip;

LP:
	       while i < Len(wire) do
	       if wire[i].o = "U" then
	       x := x + 1;
	       elsif wire[i].o = "D" then
	       x := x - 1;
	       elsif wire[i].o = "L" then
	       y := y - 1;
	       elsif wire[i].o = "R" then
	       y := y + 1;
	       end if;
	       pts := pts \union {["x"|->x, "y"|->y]};
	       end while;

end algorithm; *)
\* BEGIN TRANSLATION
CONSTANT defaultInitValue
VARIABLES Points1, Points2, i, x, y, pts, wire, pc

vars == << Points1, Points2, i, x, y, pts, wire, pc >>

Init == (* Global variables *)
        /\ Points1 = {}
        /\ Points2 = {}
        /\ i = 0
        /\ x = 0
        /\ y = 0
        /\ pts = {}
        /\ wire = defaultInitValue
        /\ pc = "ONE"

ONE == /\ pc = "ONE"
       /\ wire' = wire1
       /\ pc' = "LP"
       /\ UNCHANGED << Points1, Points2, i, x, y, pts >>

LP == /\ pc = "LP"
      /\ IF i < Len(wire)
            THEN /\ IF wire[i].o = "U"
                       THEN /\ x' = x + 1
                            /\ y' = y
                       ELSE /\ IF wire[i].o = "D"
                                  THEN /\ x' = x - 1
                                       /\ y' = y
                                  ELSE /\ IF wire[i].o = "L"
                                             THEN /\ y' = y - 1
                                             ELSE /\ IF wire[i].o = "R"
                                                        THEN /\ y' = y + 1
                                                        ELSE /\ TRUE
                                                             /\ y' = y
                                       /\ x' = x
                 /\ pts' = (pts \union {["x"|->x', "y"|->y']})
                 /\ pc' = "LP"
            ELSE /\ pc' = "Done"
                 /\ UNCHANGED << x, y, pts >>
      /\ UNCHANGED << Points1, Points2, i, wire >>

(* Allow infinite stuttering to prevent deadlock on termination. *)
Terminating == pc = "Done" /\ UNCHANGED vars

Next == ONE \/ LP
           \/ Terminating

Spec == Init /\ [][Next]_vars

Termination == <>(pc = "Done")

\* END TRANSLATION
